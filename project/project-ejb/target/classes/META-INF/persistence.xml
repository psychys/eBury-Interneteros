<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" 
	xmlns="http://xmlns.jcp.org/xml/ns/persistence" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
  <persistence-unit name="eBury_project" transaction-type="JTA">
    <jta-data-source>jdbc/sii</jta-data-source>

      <class>eBury_project.Cliente</class>
      <class>eBury_project.Cuenta_referencia</class>
      <class>eBury_project.AutorizacionEmpresaAutorizado</class>
      <class>eBury_project.DepositadaPooledReferencia</class>
      <class>eBury_project.Pooled</class>
      <class>eBury_project.Segregated</class>
      <class>eBury_project.Empresa</class>
      <class>eBury_project.Individual</class>
      <class>eBury_project.Cuenta_Fintech</class>
      <class>eBury_project.Transaccion</class>
      <class>eBury_project.Divisa</class>
      <class>eBury_project.Persona_autorizada</class>
      <class>eBury_project.Usuario</class>

    <shared-cache-mode>NONE</shared-cache-mode>
    <properties>
    	<property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
